[run]
source = .
omit =
    test_*.py
    *_test.py
    conftest.py
    venv/*
    env/*
    backup/*
    run_tests.py

[report]
fail_under = 80
show_missing = true
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:

[html]
directory = htmlcov
